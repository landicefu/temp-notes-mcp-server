{
  "name": "temp-notes-mcp-server",
  "version": "1.0.0",
  "main": "build/index.js",
  "type": "module",
  "bin": {
    "temp-notes-mcp-server": "./build/index.js"
  },
  "scripts": {
    "prepare": "npm run build",
    "build": "tsc && node -e \"require('fs').chmodSync('build/index.js', '755')\"",
    "start": "node build/index.js",
    "dev": "tsc -w"
  },
  "keywords": [
    "mcp",
    "notes",
    "temporary storage"
  ],
  "author": "",
  "license": "MIT",
  "description": "MCP server for temporary notes storage",
  "dependencies": {
    "@modelcontextprotocol/sdk": "^1.6.0",
    "fs-extra": "^11.2.0"
  },
  "devDependencies": {
    "@types/fs-extra": "^11.0.4",
    "@types/node": "^22.13.5",
    "typescript": "^5.7.3"
  }
}